    "#---- Area B----#\n",
    "import tensorflow as tf\n",
    "\n",
    "# m = tf.Variable(tf.random_normal([3,3]))  \n",
    "m = tf.constant(\n",
    "    [[ 0.20629479, -0.489871,    0.36569953] ,\n",
